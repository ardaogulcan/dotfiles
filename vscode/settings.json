{
  "workbench.colorTheme": "Agila Oceanic Next",
  "window.zoomLevel": 0,
  "window.newWindowDimensions": "maximized",
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.snippetSuggestions": "top",
  "editor.formatOnPaste": true,
  "editor.fontSize": 15,
  "editor.tabSize": 2,
  "editor.renderIndentGuides": false,
  "editor.showFoldingControls": "mouseover",
  "editor.fontFamily": "Menlo",
  "editor.lineNumbers": "on",
  "files.trimTrailingWhitespace": true,
  "workbench.settings.editor": "json",
  "files.exclude": {
    "**/bower_components": true,
    "**/build": true,
    "**/dist": false,
    "**/lib": true,
    "**/node_modules": true,
    "**/.vscode": true,
    "**/coverage": true
  },
  "emmet.syntaxProfiles": {
    "javascript": "jsx",
    "xml": {
      "attr_quotes": "single"
    }
  },
  "search.useGlobalIgnoreFiles": true,
  "workbench.editor.highlightModifiedTabs": true,
  "breadcrumbs.enabled": true,
  "search.showLineNumbers": true,
  "emmet.triggerExpansionOnTab": true,
  "eslint.packageManager": "yarn",
  "terminal.integrated.fontFamily": "Meslo LG M for Powerline",
  "terminal.integrated.fontSize": 13,
  "workbench.activityBar.visible": true,
  "workbench.colorCustomizations": {
    "sideBar.background": "#1A1E24",
    "activityBar.background": "#1A1E24",
    "statusBar.background": "#22272F",
    "titleBar.activeBackground": "#1A1E24",
    "tab.inactiveBackground": "#252A33",
    "sideBarSectionHeader.background": "#22272F",
    "list.inactiveSelectionBackground": "#353D49",
    "tab.hoverBackground": "#353D49",
    "list.hoverBackground": "#22272F",
    "list.activeSelectionBackground": "#252A33",
    "list.focusBackground": "#252A33",
    "editorGroupHeader.tabsBackground": "#1A1E24",
    "input.background": "#22272F",
    "tab.border": "#1A1E24",
    "editorOverviewRuler.border": "#1A1E24",
    "scrollbarSlider.background": "#22272F",
    "scrollbarSlider.hoverBackground": "#353D49",
    "scrollbarSlider.activeBackground": "#5C6A76",
    "scrollbar.shadow": "#111418",
    "editorLineNumber.foreground": "#5C6A7630",
    "editorWidget.background": "#22272F",
    "editorWidget.border": "#22272F00",
    "editor.lineHighlightBackground": "#22272F",
    "activityBar.foreground": "#fff",
    "activityBar.inactiveForeground": "#5C6A76",
    "statusBar.foreground": "#5C6A76"
  },
  "explorer.openEditors.visible": 0,
  "zenMode.fullScreen": true,
  "zenMode.centerLayout": true,
  "gitlens.advanced.messages": {
    "suppressLineUncommittedWarning": true
  },
  "editor.minimap.enabled": false,
  "editor.scrollBeyondLastLine": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.statusBar.feedback.visible": false
}
